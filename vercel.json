{
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run build"
        }
      }
    ]
  }